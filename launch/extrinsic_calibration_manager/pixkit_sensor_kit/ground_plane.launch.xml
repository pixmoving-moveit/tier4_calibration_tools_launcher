<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <arg name="vehicle_id" default="default"/>
  <let name="sensor_model" value="pixkit_sensor_kit"/>

  <group>
    <push-ros-namespace namespace="sensor_kit"/>
    <include file="$(find-pkg-share tier4_calibration_tools_launcher)/launch/$(var sensor_model)/ground_plane_sensor_kit.launch.xml">
      <arg name="vehicle_id" value="$(var vehicle_id)"/>
    </include>
  </group>

  <!--group>
    <push-ros-namespace namespace="sensors" />
    <include file="$(find-pkg-share tier4_calibration_tools_launcher)/launch/$(var sensor_model)/ground_plane_sensors.launch.xml">
      <arg name="vehicle_id" value="$(var vehicle_id)" />
    </include>
  </group-->
</launch>
